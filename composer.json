{
  "name": "sann4ez/laravel-holidays",
  "description": "Laravel package to get national and public holidays with multi-driver support",
  "keywords": [
    "laravel", "holidays", "calendar", "public holidays", "national holidays",
    "holiday api", "laravel package", "date utils", "working days", "non-working days",
    "sann4ez", "multi-driver", "holiday provider", "calendarific", "holidayapi", "apininjas"
  ],
  "authors": [
    {
      "name": "Kyrychuk Olexandr",
      "email": "sann4ez.dev@gmail.com",
      "role": "Architect & Developer",
      "homepage": "https://github.com/sann4ez"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Sann4ez\\Holidays\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sann4ez\\Holidays\\Tests\\": "tests"
    }
  },
  "support": {
    "email": "sann4ez.dev@gmail.com",
    "issues": "https://github.com/sann4ez/laravel-holidays/issues",
    "source": "https://github.com/sann4ez/laravel-holidays"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "validate": "composer validate --strict"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sann4ez\\Holidays\\HolidayServiceProvider"
      ],
      "aliases": {
        "Holidays": "Holidays"
      }
    }
  }
}